{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "mycred-gb-blocks/mycred-balance-history",
    "title": "Chart Balance History",
    "category": "mycred",
    "icon": "chart-line",
    "description": "A block to display a chart of balance history for MyCred points.",
    "attributes": {
      "type": {
        "type": "string",
        "default": "line"
      },
      "ctype": {
        "type": "string",
        "default": "mycred_default"
      },
      "user": {
        "type": "string",
        "default": ""
      },
      "period": {
        "type": "string",
        "default": "days"
      },
      "number": {
        "type": "string",
        "default": "10"
      },
      "order": {
        "type": "string",
        "default": "DESC"
      },
      "title": {
        "type": "string",
        "default": ""
      },
      "animate": {
        "type": "string",
        "default": "1"
      },
      "bezier": {
        "type": "string",
        "default": "1"
      },
      "labels": {
        "type": "string",
        "default": "1"
      },
      "legend": {
        "type": "string",
        "default": "1"
      },
      "width": {
        "type": "string",
        "default": ""
      },
      "height": {
        "type": "string",
        "default": ""
      }
    },
    "textdomain": "mycred",
    "editorScript": "file:./index.js",
    "render": "php:MG_Blocks\\mycred_chart_balance_block::render_block"
}